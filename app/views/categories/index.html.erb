<h1>Category</h1>
<%= link_to 'back', categorys_path, class: 'btn btn-outline-dark' %>
<%= link_to 'new', new_category_path, class: 'btn btn-outline-dark' %>
<hr>
 <ul>
     <% @categories.each do |category| %>
         <li>
         <%= category.name %>
         <%= link_to :edit, edit_category_path(category), class: 'btn btn-outline-dark'%>
         <%= button_to :delete, category_path(category), method: :delete %>
       </li>
       <% end %>
   </ul>
